<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.com.fortnite.fortnitesoundboard">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">
        <activity android:name="my.com.fortnite.fortnitesoundboard.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="my.com.fortnite.fortnitesoundboard.Ninja"
            android:label="Ninja" />
        <activity
            android:name="my.com.fortnite.fortnitesoundboard.Myth"
            android:label="Myth" />

        <activity
            android:name="my.com.fortnite.fortnitesoundboard.Daequan"
            android:label="Daequan" />

        <activity
            android:name="my.com.fortnite.fortnitesoundboard.Tim"
            android:label="Tim" />

        <activity
            android:name="my.com.fortnite.fortnitesoundboard.Pop"
            android:label=""
            android:theme="@style/AppTheme.popupTheme"/>

    </application>

</manifest>